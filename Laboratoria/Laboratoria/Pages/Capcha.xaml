<Page x:Class="Laboratoria.Pages.Capcha"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Laboratoria.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Capcha">

    <Grid Background="White" Loaded="Grid_Loaded">
        <Button x:Name="Bt_back" Content="Назад" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="51,357,0,0" Click="Bt_back_Click"/>
        <Button x:Name="Bt_send" Content="Отправить" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="293,193,0,0" Click="Bt_send_Click"/>
        <!--<Label x:Name="capcha_label" Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="330,89,0,0"/>-->
        <TextBox x:Name="enter_textBox" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Margin="279,150,0,0"/>
        <TextBox x:Name="capcha_Tb" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="279,106,0,0"/>
        <Button x:Name="Bt_new" Content="Обновить" HorizontalAlignment="Left" VerticalAlignment="Top" Width="74" Margin="446,105,0,0" Click="Bt_new_Click"/>

    </Grid>
</Page>
